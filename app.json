{
  "expo": {
    "name": "Lush",
    "slug": "lush",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/logo-black.png",
    "scheme": "lush",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mayagwright20.lush",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo-black.png",
        "backgroundImage": "./assets/images/logo-black.png",
        "monochromeImage": "./assets/images/logo-black.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "logo-black": "./assets/images/logo-black.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-black.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-video",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "84c2f4a7-2e0a-4a5e-b6de-6dfd0df503fe"
      }
    }
  }
}
